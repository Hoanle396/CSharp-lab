<Window x:Class="Learn_WDF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Learn_WDF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame x:Name="frameMain" VerticalAlignment="Stretch" NavigationUIVisibility="Visible"/>
        <Button HorizontalAlignment="Center" Name="Button1" Height="60" Width="180" Margin="-200,250,0,0" FontSize="32" Click="Button1_Click" >Page 1</Button>
        <Button HorizontalAlignment="Center" Name="Button2" Height="60" Width="180" Margin="200,250,0,0" FontSize="32" Click="Button2_Click" >Page 2</Button>
    </Grid>
</Window>
